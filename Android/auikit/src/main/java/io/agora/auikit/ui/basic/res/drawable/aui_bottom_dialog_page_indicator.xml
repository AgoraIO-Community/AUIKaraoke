<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_selected="true">
        <layer-list>

            <item android:top="10dp" android:bottom="10dp">
                <shape android:shape="rectangle">
                    <solid android:color="?attr/aui_bottomDialog_pageIndicatorColorSelected" />
                    <corners android:radius="99dp"/>
                    <size
                        android:width="10dp"
                        android:height="10dp" />
                </shape>
            </item>
        </layer-list>
    </item>
    <item>
        <layer-list>
            <item android:top="10dp" android:bottom="10dp">
                <shape android:shape="rectangle">
                    <solid android:color="?attr/aui_bottomDialog_pageIndicatorColor" />
                    <corners android:radius="99dp"/>
                    <size
                        android:width="10dp"
                        android:height="10dp" />
                </shape>
            </item>
        </layer-list>
    </item>
</selector>
